{
  "name": "@jjmhalew/angular-jwt",
  "version": "21.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angular-jwt --configuration production",
    "postbuild": "cp ./README.md ./CHANGELOG.md ./LICENSE ./dist/angular-jwt/",
    "test": "ng test",
    "test:ci": "npm run test:angular-jwt && npm run test:angular-jwt-demo",
    "test:angular-jwt-demo": "ng test angular-jwt-demo --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
    "test:angular-jwt": "ng test angular-jwt --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:types": "tsd projects/angular-jwt/src",
    "lint": "ng lint",
    "release": "npm publish dist/angular-jwt/"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^21.0.9",
    "@angular/compiler": "^21.0.9",
    "@angular/core": "^21.0.9",
    "@angular/platform-browser": "^21.0.9",
    "@angular/platform-browser-dynamic": "^21.0.9",
    "core-js": "^3.47.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.0.6",
    "@angular/cli": "^21.0.6",
    "@angular/compiler-cli": "^21.0.9",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.4.0",
    "ng-packagr": "^21.1.0",
    "prettier": "^3.7.4",
    "tsd": "^0.33.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.17"
  },
  "np": {
    "contents": "dist/angular-jwt"
  },
  "publishConfig": {
    "access": "public"
  }
}
