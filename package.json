{
  "name": "@jjmhalew/angular-jwt",
  "version": "21.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angular-jwt --configuration production",
    "postbuild": "cp ./README.md ./CHANGELOG.md ./LICENSE ./dist/angular-jwt/",
    "test": "ng test",
    "test:ci": "npm run test:angular-jwt && npm run test:angular2-jwt",
    "test:angular-jwt": "ng test angular-jwt --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
    "test:angular2-jwt": "ng test angular-jwt --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:types": "tsd projects/angular-jwt/src",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "npm publish dist/angular-jwt/"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^21.0.9",
    "@angular/compiler": "^21.0.9",
    "@angular/core": "^21.0.9",
    "@angular/platform-browser": "^21.0.9",
    "@angular/platform-browser-dynamic": "^21.0.9",
    "core-js": "^3.47.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.0.6",
    "@angular/cli": "^21.0.6",
    "@angular/compiler-cli": "^21.0.9",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.2",
    "ng-packagr": "^21.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.17"
  },
  "np": {
    "contents": "dist/angular-jwt"
  },
  "publishConfig": {
    "access": "public"
  }
}
